{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-bootstrap-switch-demo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/ngx-bootstrap-switch-demo"
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts"
            ],
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap-switch.css"
            ],
            "scripts": [],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "ngx-bootstrap-switch-demo:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "ngx-bootstrap-switch-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ngx-bootstrap-switch-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css",
              "src/assets/css/bootstrap-switch.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        }
      }
    },
    "ngx-bootstrap-switch-demo-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ngx-bootstrap-switch-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngx-bootstrap-switch-demo:serve:production"
            }
          }
        }
      }
    },
    "jw-bootstrap-switch-ng2": {
      "root": "projects/jw-bootstrap-switch-ng2",
      "sourceRoot": "projects/jw-bootstrap-switch-ng2/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/jw-bootstrap-switch-ng2/tsconfig.lib.json",
            "project": "projects/jw-bootstrap-switch-ng2/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/jw-bootstrap-switch-ng2/tsconfig.lib.prod.json",
              "project": "projects/jw-bootstrap-switch-ng2/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/jw-bootstrap-switch-ng2/src/test.ts",
            "tsConfig": "projects/jw-bootstrap-switch-ng2/tsconfig.spec.json",
            "karmaConfig": "projects/jw-bootstrap-switch-ng2/karma.conf.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "c16479b5-7a42-46bb-b8dd-07b1949317fb"
  }
}